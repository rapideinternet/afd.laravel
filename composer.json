{
  "name": "sivi/afd.laravel",
  "type": "library",
  "description": "Laravel implementation for the AFD standard",
  "keywords": [
    "laravel",
    "SIVI",
    "Financial",
    "AFD",
    "GIM",
    "SKP",
    "ADN"
  ],
  "homepage": "https://github.com/rapideinternet/afd.laravel",
  "license": "MIT",
  "authors": [
    {
      "name": "Peter Mein",
      "email": "peter@rapide.software"
    },
    {
      "name": "Lucas van der Have",
      "email": "lucas@dossierdata.nl"
    }
  ],
  "prefer-stable": true,
  "require": {
    "php": ">=7.4 || 8.*",
    "laravel/framework": "^6.0 || ^7.0 || ^8.0 || ^9.0",
    "guzzlehttp/guzzle": "^6.0 || ^7.2",
    "doctrine/dbal": "^2.5",
    "sivi/afd": "^3",
    "sivi/afd.connectors": "^2"
  },
  "require-dev": {
    "phpunit/phpunit": "~8.0",
    "phpunit/php-code-coverage": "~7.0",
    "orchestra/database": "~4.3",
    "orchestra/testbench": "~4.18"
  },
  "autoload": {
    "psr-4": {
      "SIVI\\LaravelAFD\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "SIVI\\LaravelAFD\\Providers\\AfdServiceProvider"
      ]
    }
  }
}
